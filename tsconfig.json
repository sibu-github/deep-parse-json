{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "module": "CommonJS",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist", "__tests__"]
}
